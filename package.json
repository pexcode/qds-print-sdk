{
  "name": "@pexcode/qds-print-sdk",
  "version": "2.2.1",
  "description": "This is a TypeScript library published under an pexcode to help users and developers to use quickdeliverysystem print.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module", 
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Mussab Muhaimeed",
  "license": "MIT",
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly",
    "build": "tsc",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.5.0",
    "undici-types": "^5.1.0",
    "@types/jsbarcode": "^3.11.3",
    "@types/qrcode": "^1.5.4",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "jsbarcode": "^3.11.6",
    "qrcode": "^1.5.4"
  },
  "keywords": [
    "qrcode",
    "barcode",
    "printing",
    "typescript",
    "sdk",
    "pexcode",
    "quickdeliverysystem"
  ]
}
